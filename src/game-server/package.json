{
    "private": true,
    "name": "my-app",
    "version": "0.17.0",
    "description": "npm init template for bootstrapping an empty Colyseus project",
    "type": "module",
    "main": "build/index.js",
    "engines": {
        "node": ">= 20.9.0"
    },
    "scripts": {
        "start": "node ./build/src/index.js",
        "dev": "tsx watch src/index.ts",
        "loadtest": "tsx loadtest/example.ts --room my_room --numClients 2",
        "build": "npm run clean && tsc -p tsconfig.build.json",
        "clean": "rimraf build",
        "test": "mocha -r tsx test/**.test.ts --exit --timeout 15000"
    },
    "author": "",
    "license": "UNLICENSED",
    "bugs": {
        "url": "https://github.com/colyseus/create-colyseus/issues"
    },
    "homepage": "https://github.com/colyseus/create-colyseus#readme",
    "devDependencies": {
        "@colyseus/loadtest": "^0.17.7",
        "@colyseus/sdk": "^0.17.31",
        "@colyseus/testing": "^0.17.10",
        "@types/express": "^4.17.1",
        "@types/mocha": "^10.0.1",
        "mocha": "^11.7.5",
        "rimraf": "^5.0.0",
        "tsx": "^4.21.0",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "@babylonjs/core": "^8.51.0",
        "@babylonjs/havok": "^1.3.11",
        "@babylonjs/loaders": "^8.51.0",
        "@babylonjs/materials": "^8.51.0",
        "@colyseus/monitor": "^0.17.7",
        "@colyseus/schema": "^4.0.0",
        "@colyseus/tools": "^0.17.0",
        "colyseus": "^0.17.6",
        "express": "^4.18.2",
        "express-basic-auth": "^1.2.1"
    },
    "imports": {
        "#gameEngine/*": {
            "node": "./build/src/gameEngine/*",
            "default": "./src/gameEngine/*"
        },
        "#rooms/*": {
            "node": "./build/src/rooms/*",
            "default": "./src/rooms/*"
        },
        "#schema/*": {
            "node": "./build/src/rooms/schema/*",
            "default": "./src/rooms/schema/*"
        },
        "#public/*": {
            "node": "./build/public/*",
            "default": "./public/*"
        },
        "#entities/*": {
            "node": "./build/src/rooms/entities/*",
            "default": "./src/rooms/entities/*"
        },
        "#types/*": {
            "node": "./build/src/types/*",
            "default": "./src/types/*"
        }
    }
}
