  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && NODE_ENV=production node --env-file-if-exists=.env dist/src/index.js",
    "dev": "npm run build && NODE_ENV=development node --env-file-if-exists=.env dist/src/index.js",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "build": "npm run prisma:migrate && npm run prisma:generate && tsc -p tsconfig.json",
    "clean": "rm -rf ../../dist/auth",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
