{
  "name": "user-management",
  "version": "1.0.0",
  "description": "",
  "author": "Artem Mysiv",
  "license": "ISC",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "npm run prisma:migrate && npm run prisma:generate && NODE_ENV=production node --env-file-if-exists=.env dist/src/index.js",
    "dev": "npm run prisma:migrate && npm run prisma:generate && NODE_ENV=development node --env-file-if-exists=.env dist/src/index.js",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf ./dist",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.19.30",
    "@vitest/coverage-v8": "^2.1.8",
    "dotenv": "^16.6.1",
    "prisma": "^7.3.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@fastify/type-provider-typebox": "^6.1.0",
    "@prisma/adapter-better-sqlite3": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@sinclair/typebox": "^0.34.48",
    "axios": "^1.13.6",
    "fastify": "^5.7.4",
    "pino-pretty": "^13.1.3",
    "zod": "^4.3.6"
  }
}
